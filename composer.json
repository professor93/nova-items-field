{
    "name": "uzbek/nova-items-field",
    "description": "A Laravel Nova items field.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=8.0",
        "laravel/nova": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "Uzbek\\ItemsField\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://laravel-nova.site"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Uzbek\\ItemsField\\FieldServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
